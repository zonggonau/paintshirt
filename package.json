    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "classcat": "^5.0.4",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "lodash.shuffle": "^4.2.0",
    "next": "16.1.2",
    "postgres": "^3.4.8",
    "printful-request": "^2.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4",
    "@types/lodash.shuffle": "^4.2.7",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
